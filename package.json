  "description": "A powerful JavaScript/TypeScript dependency analyzer that traces module relationships and identifies unused code",
  "main": "index.js",
  "type": "module",
  "bin": {
    "treetracr": "./index.js"
  },
  "scripts": {
    "start": "node index.js"
  },
  "keywords": [
    "static-analysis",
    "dependencies",
    "code-quality",
